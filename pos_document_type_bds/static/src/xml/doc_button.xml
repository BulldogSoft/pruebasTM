<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
	<t t-name="DoctypeButton" owl="1">
		<div class="button doctypemethod" t-on-click="trigger('new-doctype-line',props.doctype)" >
