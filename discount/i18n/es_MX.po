#. module: discount
#: model:mail.template,body_html:discount.email_template_sale_reject
msgid ""
"\n"
"            <p>Dear Sir/Madam,</p>\n"
"                <br/>\n"
"                <p>Discount approval for the sale quoataion ${ctx['name']} "
"has been rejected.</p>\n"
"                <p> Reject Reason: ${ctx['reject_reason']}\n"
"                <br/>\n"
"                <p>Thank You</p>    \n"
"            "
msgstr ""
"\n"
"            <p>Estimado usuario,</p>\n"
"                <br/>\n"
"                <p>La aprobación del descuento para la Cotización "
"${ctx[‘name’]} ha sido rechazada.</p>\n"
"                <p> Razón del rechazo: ${ctx['reject_reason']}\n"
"                <br/>\n"
"               <p>Gracias</p>   \n"
"            "

#. module: discount
#: model:mail.template,body_html:discount.email_template_sale_approval
msgid ""
"\n"
"            <p>Dear Sir/Madam,</p>\n"
"                <br/>\n"
"                <p>Kindly approve the Sale quotaion ${ctx['name']} for the "
"customer ${ctx['partner_name']}</p>\n"
"                <br/>\n"
"                <p>Thank You</p>    \n"
"            "
msgstr ""
"\n"
"            <p>Estimado Encargado de descuentos,</p>\n"
"                <br/>\n"
"                <p>Por favor, revise el descuento para la cotización "
"${ctx['name']} del cliente ${ctx['partner_name']}</p>\n"
"                <br/>\n"
"                <p>Gracias</p>    \n"
"            "

