<odoo>
    <record id="view_company_form_contribuyente_type" model="ir.ui.view">
        <field name="name">view.company.form.fiscal.position.type</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form" />
        <field name="arch" type="xml">
            <xpath expr="//div[hasclass('o_address_format')]" position="after">
                <field name="default_contribuyente_type" />
            </xpath>
        </field>
    </record>
</odoo>
